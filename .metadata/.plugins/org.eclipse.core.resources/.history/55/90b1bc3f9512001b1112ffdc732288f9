#include "OctomapTest.hpp"

using namespace octomap_test;

int main(int argc, char** argv)
{
	//printf("%d \n", argc);
	  if (argc != 3 )
	  {
	    printf("usage %s <datpath> <sceneidx>", argv[0] );
	    exit(-1);
	  }

	std::stringstream ss(argv[2]);
	uint32_t uSceneIdx = 0;
	ss >> uSceneIdx ;

	OctomapTest octest(argv[1], uSceneIdx);

	octest.save_occ_cells() ;
	octest.save_free_cells() ;

	return 0;
}

